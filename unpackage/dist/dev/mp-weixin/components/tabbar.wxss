
.cu-bar.tabbar .action{
	margin: 15rpx;
}
.iconfont{
	width: 50rpx;
	font-size: 50rpx;
	margin-bottom: 5rpx;
}
.action>view{
	text-align: center;
}
.gradient{
	background-image: -webkit-gradient(linear, left top, left bottom, from(rgb(255,109,0)), to(rgb(255,200,0)));
	background-image: -o-linear-gradient(top, rgb(255,109,0), rgb(255,200,0));
	background-image: linear-gradient(to bottom, rgb(255,109,0), rgb(255,200,0));
	-webkit-background-clip: text;
	color: transparent;
	-webkit-text-fill-color: transparent;
	text-fill-color: transparent;
}


